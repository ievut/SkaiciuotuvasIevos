    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="mygtukasPlius,mygtukasDalinti,mygtukas1,mygtukas0,
        mygtukasLygu,mygtukasTrinti,mygtukas8,mygtukas4,mygtukas5,mygtukas9,mygtukas2,mygtukas7,
