        android:layout_alignRight="@+id/edt1"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@+id/mygtukas3"
        android:text="+"
        android:textColor="@color/design_default_color_secondary"
