        android:layout_height="wrap_content"
        android:layout_below="@+id/mygtukas4"
        android:layout_marginLeft="10dp"
        android:layout_toLeftOf="@+id/mygtukas2"
        android:text="7"
        android:textSize="20sp"
        android:textStyle="bold" />


    <Button
        android:id="@+id/mygtukas8"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mygtukas5"
        android:layout_alignStart="@+id/mygtukas5"
        android:layout_alignLeft="@+id/mygtukas5"
        android:text="8"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/mygtukas9"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mygtukas6"
        android:layout_alignStart="@+id/mygtukas6"
        android:layout_alignLeft="@+id/mygtukas6"
